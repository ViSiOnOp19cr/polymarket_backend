    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.10.1",
    "typescript": "^5.8.3",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.4",
    "express": "^5.1.0",
    "tsx": "^4.7.0"
  }
